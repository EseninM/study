package main

import (
	"fmt"
)

// Функция для нахождения простых чисел в заданном диапазоне
func простыеЧисла(min, max int) []int {
	простые := []int{}

	// Перебираем числа в заданном диапазоне
	for число := min; число <= max; число++ {
		if простое(число) {
			простые = append(простые, число)
		}
	}

	return простые
}

// Функция для проверки, является ли число простым
func простое(число int) bool {
	if число <= 1 {
		return false
	}
	for i := 2; i*i <= число; i++ {
		if число%i == 0 {
			return false
		}
	}
	return true
}

func main() {
	мин := 11
	макс := 20
	fmt.Printf("Простые числа в диапазоне от %d до %d: %v\n", мин, макс, простыеЧисла(мин, макс))
}
